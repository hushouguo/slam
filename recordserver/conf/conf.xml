<?xml version="1.0"?>
<root>
	<log level = "0" dir = ".logs" autosplit_day = "true" autosplit_hour = "false"/> <!-- 0, 1, 2, 3, 4 -->
	<limit stack_size = "16777216" max_files = "4000"/> <!-- 16M -->
	<service address = "0.0.0.0" port = "9990"/>
	<db>
		<slot value = "8"/>
		<sync interval = "100"/>
		<conf filename = "conf/db.xml"/>
	</db>
</root>
